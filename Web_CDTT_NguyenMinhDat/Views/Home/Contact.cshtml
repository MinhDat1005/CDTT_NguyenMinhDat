<div id="contact" class="section">
	<!-- container -->
	<div class="container">
		<!-- row -->
		<div class="row">
			<!-- Contact Form -->
			<div class="col-md-6">
				<div class="contact-form">
					<p>Get in Touch with Us</p>
					<form>
						<input class="input" type="text" placeholder="Enter Your Name" >
						<input class="input" type="email" placeholder="Enter Your Email" >
						<textarea class="input" placeholder="Your Message" ></textarea>
						<button class="contact-btn"><i class="fa fa-paper-plane"></i> Send Message</button>
					</form>
					
				</div>
			</div>
			<!-- /Contact Form -->
			<!-- Map -->
			<div class="col-md-6">
				<div class="contact-map">
					<p>Our Location</p>
					<iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3153.450784552451!2d-122.41941538468185!3d37.77492977975938!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x8085814d5f5763bf%3A0x97b8f0d4709af9!2sSan%20Francisco%2C%20CA%2C%20USA!5e0!3m2!1sen!2s!4v1600000000000!5m2!1sen!2s"
							width="100%"
							height="450"
							style="border:0;"
							allowfullscreen=""
							loading="lazy">
					</iframe>
				</div>
			</div>
			<!-- /Map -->
		</div>
		<!-- /row -->
	</div>
	<!-- /container -->
</div>
